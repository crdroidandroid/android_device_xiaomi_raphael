# Wakeup source stats
genfscon sysfs /devices/platform/goodix_ts.0/wakeup                                                                                                u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/18800000.qcom,icnss/wakeup                                                                                    u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/890000.i2c/i2c-0/0-0010/890000.i2c:qcom,smb1390@10:qcom,charge_pump/wakeup                                    u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/a84000.i2c/i2c-2/2-0028/wakeup                                                                                u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/171c0000.slim/tavil-slim-pgd/wakeup                                                                           u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/vendor/vendor:extcon_usb1/wakeup                                                                                  u:object_r:sysfs_wakeup:s0

# KProfiles
genfscon sysfs /module/kprofiles/parameters/auto_kp                                                                                                 u:object_r:sysfs_kprofiles:s0
genfscon sysfs /kernel/kprofiles/kp_mode                                                                                                            u:object_r:sysfs_kprofiles:s0

# CSC DC Dimming
genfscon sysfs /devices/platform/soc/soc:qcom,dsi-display-primary/dc_dim                                                                            u:object_r:vendor_sysfs_graphics:s0

# HBM
genfscon sysfs /devices/platform/soc/soc:qcom,dsi-display-primary/hbm                                                                               u:object_r:vendor_sysfs_graphics:s0

# UDFPS Sensor
genfscon sysfs /devices/platform/goodix_ts.0/udfps_pressed                                                                                          u:object_r:vendor_sysfs_sensors:s0
genfscon sysfs /devices/platform/goodix_ts.0/udfps_enabled                                                                                          u:object_r:vendor_sysfs_sensors:s0

# Double Tap Sensor
genfscon sysfs /devices/platform/goodix_ts.0/double_tap_pressed                                                                                     u:object_r:vendor_sysfs_sensors:s0
genfscon sysfs /devices/platform/goodix_ts.0/double_tap_enabled                                                                                     u:object_r:vendor_sysfs_sensors:s0
